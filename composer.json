{
    "name": "tishmalo/escalation-matrix",
    "description": "Escalation Matrix Package for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "error-handling",
        "escalation",
        "exception-handler",
        "notifications",
        "support-tickets",
        "sms-alerts",
        "error-monitoring"
    ],
    "authors": [
        {
            "name": "Tishmalo",
            "email": "tishmalo99@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/mail": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Tishmalo\\EscalationMatrix\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tishmalo\\EscalationMatrix\\EscalationServiceProvider"
            ],
            "aliases": {
                "Escalation": "Tishmalo\\EscalationMatrix\\Facades\\Escalation"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}